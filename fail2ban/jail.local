[DEFAULT]

# "ignoreip" can be an IP address, a CIDR mask or a DNS host
ignoreip = 127.0.0.1/8 
bantime  = 900
maxretry = 3

backend = auto
#banaction = iptables-multiport


[LogDistro]
#backend=polling
backend=gamin
enabled = true
filter=newbans
logpath = /opt/logdistro/logs/newbans2.log
maxretry = 1
datepattern = %%Y-%%m-%%d %%H:%%M:%%S
